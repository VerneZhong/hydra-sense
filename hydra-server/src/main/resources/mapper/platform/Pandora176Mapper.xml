<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hydra.server.modules.platform.mapper.Pandora176Mapper">
    
    <resultMap type="com.hydra.server.modules.platform.entity.Pandora176" id="Pandora176Result">
        <result property="id"    column="id"    />
        <result property="fileName"    column="file_name"    />
        <result property="humidity"    column="humidity"    />
        <result property="time"    column="time"    />
    </resultMap>

    <sql id="selectHumidityPandora176Vo">
        select id, file_name, humidity, time from Pandora_176
    </sql>

    <select id="selectPandora176List"  resultMap="Pandora176Result">
        <include refid="selectHumidityPandora176Vo"/>
        <where>
            deleted = 0
        </where>
    </select>
    
    <select id="selectPandora176ById" parameterType="Long" resultMap="Pandora176Result">
        <include refid="selectHumidityPandora176Vo"/>
        where id = #{id}
    </select>

    <select id="selectPandoraList" resultMap="Pandora176Result">
        <include refid="selectHumidityPandora176Vo"/>
        <where>
            time <![CDATA[ <= ]]> #{endTime,jdbcType=VARCHAR}
            and time <![CDATA[ >= ]]> #{startTime,jdbcType=VARCHAR}
        </where>
    </select>
</mapper>